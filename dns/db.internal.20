$ORIGIN .
$TTL 600  ; 10 minutes
cdh-util.internal  IN SOA  dnsserver.cdh-util.internal. hostmaster.cdh-util.internal. (
        10         ; serial
        600        ; refresh (10 minutes)
        60         ; retry (1 minute)
        604800     ; expire (1 week)
        600        ; minimum (10 minutes)
        )
        NS  dnsserver.cdh-util.internal.

$ORIGIN cdh-util.internal.
dnsserver    A  10.2.20.4
